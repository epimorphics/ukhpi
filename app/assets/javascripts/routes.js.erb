/** Simple routes table to export path information from Rails */

modulejs.define( "routes", [],
function() {
  "use strict";

  return {
    explore: "<%= Rails.application.config.relative_url_root%><%= Rails.application.routes.url_helpers.explore_path %>",
    newExploration: "<%= Rails.application.config.relative_url_root%><%= Rails.application.routes.url_helpers.new_exploration_path %>",
    publicPath: "<%= Rails.application.config.relative_url_root%>/"
  };
} );
